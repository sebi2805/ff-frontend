{
  "name": "ff-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build-qa": "env-cmd -f ./.env.qa next build",
    "build-development": "env-cmd -f ./.env.development next build",
    "build-main": "env-cmd -f ./.env.production next build",
    "start-development": "env-cmd -f ./.env.development node_modules/next/dist/bin/next start -p 10000",
    "start-qa": "env-cmd -f ./.env.qa node_modules/next/dist/bin/next start -p 10001",
    "start-main": "env-cmd -f ./.env.production node_modules/next/dist/bin/next start -p 10002",
    "lint": "next lint",
    "start": "next start",
    "export": "next export"
  },
  "dependencies": {
    "autoprefixer": "^10.4.20",
    "next": "13.4.7",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "env-cmd": "^10.1.0",
    "eslint": "^8",
    "eslint-config-next": "13.4.7",
    "postcss": "^8",
    "tailwindcss": "3.3.0",
    "typescript": "^5"
  }
}
